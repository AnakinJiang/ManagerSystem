<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edu.zjut.po.ChatGroup" table="ChatGroupInfo" catalog="helpdb">
        <id name="listID" type="java.lang.String">
            <column name="listID" length="32"/>
            <generator class="uuid"/>
        </id>
        <property name="groupID" type="java.lang.String"/>
        <property name="memberID" type="java.lang.String"/>
    </class>
</hibernate-mapping>